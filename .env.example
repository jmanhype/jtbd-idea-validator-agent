# JTBD Agent Environment Configuration

# LLM API Keys (add your own)
GEMINI_API_KEY=
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
GROQ_API_KEY=

# JTBD Agent Configuration
JTBD_MODEL=gpt-4
JTBD_DOUBLE_JUDGE=1
JTBD_PORT=8000
JTBD_HOST=0.0.0.0

# DSPy Configuration
JTBD_DSPY_MODEL=claude-3-5-sonnet-20240620
DSPY_CACHEDIR=.dspy_cache
DSPY_NOTEBOOKS=false

# Performance Settings
JTBD_LLM_TEMPERATURE=0.2
JTBD_LLM_SEED=42
JTBD_MAX_TOKENS=4000

# Cost Control
JTBD_USE_CACHE=true
JTBD_CACHE_TTL_HOURS=24